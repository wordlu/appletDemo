.content-page {
  display: flex;
  flex-direction: column;

  padding-top: 68rpx;
  padding-left: 80rpx;
  padding-right: 80rpx;
}
.title {
  margin-bottom: 80rpx;
  font-family: PingFangSC-Medium;
  font-size: 22px;
  color: #0D0D0D
}
.section {
  margin-bottom: 80rpx;
}
.section .title {
  font-family: PingFangSC-Regular;
  font-size: 16px;
  color: #737373;
  margin-bottom: 38rpx;
  align-items: center;
}
.section input {
  border-bottom: 1px solid #E0E0E0;
  font-family: SFProText-Regular;
  font-size: 14px;
  color: #D9D9D9;
  margin-top: 38rpx;
  padding-bottom:22rpx; 
}
.section .test {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  margin-bottom: -35rpx;
}
text.sendMsg {
  background: #80AEFF;
  border-radius: 100px;
  width: 250rpx;
  height: 64rpx;
  text-align: center;
  line-height: 64rpx;
  font-family: PingFangSC-Medium;
  font-size: 14px;
  color: #fff;
}
text.sendMsged {
  background:  #F2F2F2;
  border-radius: 100px;
  width: 250rpx;
  height: 64rpx;
  text-align: center;
  line-height: 64rpx;
  font-family: PingFangSC-Medium;
  font-size: 14px;
  color: #B2B2B2;
}
.login {
  background-image: linear-gradient(90deg, #9DC0FF 0%, #6E9CEE 100%);
  border-radius: 24px;
  width: 590rpx;
  height: 96rpx;
  line-height: 96rpx;

  font-family: SFProText-Medium;
  font-size: 16px;
  color: #FFFFFF;
}